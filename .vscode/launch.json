{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/server_ts",
      "name": "Launch Program",
      "program": "${workspaceFolder}/server_ts/index.ts",
      "preLaunchTask": "tsc build",
      "outFiles": [
        "${workspaceFolder}/server_ts/dist/**/*.js"
      ]
    }
  ]
}